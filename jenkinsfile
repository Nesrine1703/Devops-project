pipeline{
    
    agent any


stages{
      stage('Build') {
            steps {
                // Build the source code  with Maven
                sh 'mvn clean compile'
            }
        }
      stage('clean workspace')
        {
            steps{
                cleanWs()
                 }
        } 
    stage('pulling the code from GITHUB'){
        steps{
            git branch: 'MayssaOUERFELLI',
            url: 'https://github.com/Nesrine1703/Devops-project'
        }}

        stage('SonarQube'){
            steps{
                sh 'mvn sonar:sonar  -Dsonar.host.url=http://192.168.192.132:9000   -Dsonar.login=admin -Dsonar.password=sonar' 
            }
        }
        } 
}
